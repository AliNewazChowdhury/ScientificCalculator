            android:name=".trigonometricCalculationActivity"
            android:label="@string/title_activity_trigonometric_calculation"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".TriCalculationActivity" />
        <activity android:name=".splash" />
        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity android:name=".VectorActivity" />
        <activity android:name=".constants" />
        <activity
            android:name=".History.HistoryActivity"
            android:label="History"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".conversion" />
        <activity android:name=".length" />
        <activity android:name=".width" />
